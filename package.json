{
  "name": "vscode-wechat",
  "displayName": "vscode-wechat",
  "description": "Wechat WeApp support for VSCode",
  "version": "0.0.1",
  "publisher": "coderfee",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "qiniu": "^6.1.13",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
