{
  "name": "qiniu-fig-bed",
  "displayName": "qiniu-fig-bed",
  "description": "Upload files to qiniu in VSCode",
  "version": "0.0.1",
  "publisher": "coderfee",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "qiniu",
    "七牛",
    "图床"
  ],
  "author": {
    "name": "coderfee",
    "email": "coderfee@outlook.com",
    "url": "http://coderfe.cn"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=coderfee.qiniu-fig-bed",
  "repository": {
    "url": "https://github.com/coderfe/qiniu-fig-bed.git",
    "type": "git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.qiniu",
        "key": "ctrl+q n",
        "mac": "cmd+q n",
        "linux": "ctrl+q n",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "qiniu config",
      "properties": {
        "qiniu.enable": {
          "type": "boolean",
          "default": false,
          "description": "是否开启七牛上传插件"
        },
        "qiniu.AccessKey": {
          "type": "string",
          "default": "",
          "description": "必填，七牛 AccessKey"
        },
        "qiniu.SecretKey": {
          "type": "string",
          "default": "",
          "description": "必填，七牛 SecretKey"
        },
        "qiniu.bucket": {
          "type": "string",
          "default": "",
          "description": "必填，上传到的空间名称"
        },
        "qiniu.remotePath": {
          "type": "string",
          "default": "",
          "description": "文件上传到空间后的路径"
        },
        "qiniu.domin": {
          "type": "string",
          "default": "",
          "description": "七牛空间域名"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "qiniu": "^6.1.13",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}