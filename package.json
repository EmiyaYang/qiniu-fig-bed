{
  "name": "vscode-wechat",
  "displayName": "vscode-wechat",
  "description": "Wechat WeApp support for VSCode",
  "version": "0.0.1",
  "publisher": "coderfee",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.qiniu",
        "key": "ctrl+q n",
        "mac": "cmd+q n",
        "linux": "ctrl+q n",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "qiniu config",
      "properties": {
        "qiniu.enable": {
          "type": "boolean",
          "default": false,
          "description": "是否开启七牛插件"
        },
        "qiniu.AccessKey": {
          "type": "string",
          "default": "",
          "description": "七牛 AccessKey"
        },
        "qiniu.SecretKey": {
          "type": "string",
          "default": "",
          "description": "七牛 SecretKey"
        },
        "qiniu.bucket": {
          "type": "string",
          "default": "",
          "description": "七牛上传的空间"
        },
        "qiniu.remotePath": {
          "type": "string",
          "default": "",
          "description": "文件上传的路径"
        },
        "qiniu.domin": {
          "type": "string",
          "default": "",
          "description": "七牛空间域名"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "qiniu": "^6.1.13",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}